{
  "engines": {
    "node": ">=21"
  },
  "name": "chunky-list",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "yarn tsc -b && yarn vite build",
    "dev": "yarn vite",
    "format": "yarn prettier --write \"**/*.{js,json,jsx,ts,tsx}\"",
    "format-assert": "yarn prettier --check \"**/*.{js,json,jsx,ts,tsx}\"",
    "lint": "yarn eslint .",
    "preview": "yarn vite preview",
    "start": "npx serve dist",
    "test": "yarn jest",
    "type-check": "yarn tsc -noEmit",
    "validate": "yarn format-assert && yarn lint && yarn type-check && yarn test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@types/react-beautiful-dnd": "^13.1.8",
    "http-server": "^14.1.1",
    "react": "^19.1.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/js": "^9.9.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-jest": "^29.7.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.3.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  },
  "main": "index.js",
  "author": "Dylan Hassinger <dylan.hassinger@milliporesigma.com>",
  "license": "MIT"
}
